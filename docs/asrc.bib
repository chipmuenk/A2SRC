@Article{Aldroubi1992,
  Title                    = {Cardinal spline filters: Stability and convergence to ideal sinc interpolator},
  Author                   = {A. Aldroubi, M. Unser, M. Eden},
  Journal                  = {Signal Processing},
  Year                     = {1992},
  Pages                    = {127-138},
  Volume                   = {28},

  Abstract                 = {In this paper, we provide an interpretation of polynomial spline interpolation as a continuous filtering process. We 
prove that the frequency responses of the cardinal spline filters converge to the ideal lowpass filter in all Lfnorms with 
1 ~<p < + ~ as the order of the spline tends to infinity. We provide estimates for the resolution errors and the interpolation 
errors of the various filters. We also derive an upper bound for the error associated with the reconstruction of bandlimited 
signals using polynomial splines.},
  File                     = {:Interpolation\\Cardinal spline filters - Stability and convergence to the .pdf:PDF},
  Owner                    = {Floh},
  Timestamp                = {2012.07.26}
}

